@{
    ViewData["Title"] = "Detail Material";
    var detil = ViewBag.DataMaterial;
}
<!--begin::Main Content-->
<div class="row">
    <div class="col-lg-12">
        <div class="card card-custom card-sticky" id="kt_page_sticky_card">
            <div class="card-header">
                <h3 class="card-title">@ViewData["Title"]</h3>
            </div>
            <div class="card-body">
                <table class="table table-borderless">
                    <tr>
                        <th width="30%">Material ID</th>
                        <td>@detil.material_id</td>
                    </tr>
                    <tr>
                        <th width="30%">Material Plnt </th>
                        <td>@detil.material_plnt</td>
                    </tr>
                    <tr>
                        <th width="30%">Description</th>
                        <td>@detil.description</td>
                    </tr>
                </table>
            </div>
        </div>
    </div>
</div>
<!--end::Main Content-->